<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tutorlk">

    <dist:module dist:instant="true" />

    <application
        android:usesCleartextTraffic="true"
        android:name=".Home"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".userProfile"></activity>
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".Tutor_form" />
        <activity android:name=".Choice" />
        <activity android:name=".Phone_no_verify" />
        <activity
            android:name=".Login"
            android:screenOrientation="portrait" />

        <activity android:name=".contentUploadForm" />
        <activity android:name=".student_portal" />
        <activity android:name=".Tutor"
            android:screenOrientation="portrait"
            />

        <activity android:name=".tutorProfile"
                  android:screenOrientation="portrait"

            />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>